{
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "ESNext",
		"allowJs": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"strictNullChecks": true,
		"lib": ["DOM", "ES5", "ES6", "ES7"],
		"types": ["obsidian-typings", "uniformize"]
	},
	"include": ["**/*.ts"]
}
